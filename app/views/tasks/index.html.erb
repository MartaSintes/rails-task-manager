<h2>List all Tasks</h2>

<ul>

<% @tasks.each do |task|%>
<li> <%= link_to task.title, task_path(task.id) %> </li>
<%= task.title %><br><%= link_to "Delete", task_path(task), method: :delete, data:{confirm:'Are you sure you want to delete this task?'} %>
<% end %>
</ul>
<%= link_to "Add a task", tasks_new_path %>

